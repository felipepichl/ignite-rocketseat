    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "prisma": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.8",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.4.0",
